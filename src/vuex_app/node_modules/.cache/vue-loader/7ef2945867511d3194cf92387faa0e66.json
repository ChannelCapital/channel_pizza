                        </div>\n                        <div class=\"col-md-6\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" class=\"form-control\" placeholder=\"Your Password *\" value=\"\" v-model = \"form.password\"/>\n                            </div>\n                           \n                        </div>\n                        <div class=\"row\">\n                        <div class=\"col-md-6\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" class=\"form-control\" placeholder=\"First Name *\" value=\"\" v-model=\"form.firstname\"/>\n                            </div>\n                        </div>\n                        <div class=\"col-md-6\">\n                            <div class=\"form-group\">\n                                <input type=\"text\" class=\"form-control\" placeholder=\"Last Name *\" value=\"\" v-model = \"form.lastname\"/>\n                            </div>   \n                        </div>\n                    </div>\n                    <div class=\"row\"> </div>\n                    <div class=\"note\"> Adress Information </div>\n                        <div class=\"col-md-6\">\n                            <div class=\"form-group\">\n                                <input autocomplete = \"off\" v-model=\"code\" type=\"text\" class=\"form-control\" placeholder=\"Post Code *\" @blur=\"postCodeSearch\"/>\n                            </div>\n                            <div class=\"form-group\" style=\"margin:10% 25\">\n                                \n                                <ejs-autocomplete  class=\"form-control\" :dataSource='sample' :fields='dataFields_country' placeholder=\"   Country\" v-model=\"country\"> </ejs-autocomplete>\n\n                            </div>\n                        </div>\n                        <div class=\"col-md-6\">\n                            <div  style=\"margin:10% 25\">\n                                 <ejs-autocomplete class=\"form-control\" :dataSource='sample' :fields='dataFields_region' placeholder=\"   Region\" v-model=\"region\"> </ejs-autocomplete>\n                            </div>\n                            <div class=\"form-group\">\n                                <input type=\"text\" class=\"form-control\" placeholder=\"Adress Line 1\" value=\"\" v-model=\"adress_line\"/>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"row\"> \n                        \n                    </div>\n\n                    <button type=\"submit\" class=\"btnSubmit\">Submit</button>\n                </div>\n                </form>\n            </div>\n        </div>\n\n\n</template>\n\n<script>\nimport axios from 'axios'\nimport {mapActions} from 'vuex'\nimport RegistrationDialogue from '../components/RegistrationDialogue'\nimport {mapState} from 'vuex'\nexport default {components: {\n        RegistrationDialogue\n    },\n\n    \n\n    data() {\n\n        return {\n\n        code: '',\n        postcodes: [],\n        postcode:'',\n        region:'',\n        country:'',\n        countries:[],\n        filteredCountry:'',\n        dataCountry:'',\n        sample: [],\n        dataFields_country:{value:'country'},\n        dataFields_region:{value:'region'},\n        adress_line:'',\n        form:{\n          username:'',\n          password:'',\n          address:'',\n          firstname:'',\n          lastname:''\n          \n        },\n        simpleDialog : false\n\n        }\n       \n\n    },\n    computed: {\n        ...mapState({\n        authorized: state => state.auth.user\n      \n      })\n      },\n    methods: {\n    \n    postCodeSearch: function() {\n    \n      axios\n      .get(\"https://api.postcodes.io/postcodes/\" + this.code)\n      .then(response => {\n        \n        this.postcodes[0] = response.data.result;\n        this.sample = this.postcodes\n        \n      }).catch(error=>{\n       console.log(error)\n })\n      },\n\n      ...mapActions({\n         register: 'auth/register'\n      \n    }),\n       \n  showAlert() {\n    const options = {title: 'Missing Fields', size: 'sm'}\n    this.$dialogs.alert('Please fulfill all fields', options)\n    .then(res => {\n      console.log(res) // {ok: true|false|undefined}\n    })\n  },\n\n\n        submit(){ \n          \n            this.form.address = this.code + \" \" + this.country + \" \" + this.region + \" \" + this.adress_line\n            if(!!this.form.address &&!!this.form.username && !!this.form.password) {\n            this.register(this.form).then(() => {\n                this.dialog();\n            }) } \n            else \n            {\n                this.showAlert()\n            }\n              \n            \n          },\n          dialog() {\n              if(this.authorized) {\n                 this.simpleDialog = true\n                 \n             }\n          }\n    \n}\n}\n</script>\n\n<style>\n@import url(https://cdn.syncfusion.com/ej2/material.css);\n</style>"]}]}