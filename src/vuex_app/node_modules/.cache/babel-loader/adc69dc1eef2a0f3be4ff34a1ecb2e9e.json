AMA;AACA;AACA;AACA;AACA,IAAA,QAVA,sBAUA;AAEA,UAAA,KAAA,UAAA,EAAA;AACA,aAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA,OAJA,MAIA;AACA,aAAA,WAAA,GAAA,IAAA;AACA;AAEA,KApBA;;AAsBA;AACA;AACA;AACA;AACA,IAAA,MA1BA,oBA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA,IAAA,CAAA,YAAA;AACA,kBAAA,KAAA,CAAA,QAAA;AACA,iBAFA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AApCA;AApBA,CAAA","sourcesContent":["<template>\n <div class=\"container register-form\">\n\n  <form @submit.prevent=\"submit\">\n  <div class=\"form-group\">\n    <label for=\"exampleInputEmail1\">Username</label>\n    <input id=\"username\" type=\"username\" class=\"form-control\" aria-describedby=\"emailHelp\" placeholder=\"Enter Username\" v-model = \"form.username\">\n    <small id=\"emailHelp\" class=\"form-text text-muted\">We'll never share your information with anyone else.</small>\n  </div>\n  <div class=\"form-group\">\n    <label for=\"exampleInputPassword1\">Password</label>\n    <input id=\"password\" type=\"password\" class=\"form-control\"  placeholder=\"Password\" v-model=\"form.password\">\n  </div>\n \n  <button type=\"submit\" id=\"submit\" class=\"btn btn-primary\">Submit</button>\n</form>\n<br>\n<div v-if=\"loginfailed\">\n  Login failed : Login information is incorrect\n</div>\n\n        </div>\n\n</template>\n\n<script>\n\nimport {mapActions} from 'vuex'\nimport {mapState} from 'vuex'\n\n/**\n * This is a login page for user signin. Vuex store is used for keeping product and user information. \n * @author Enis Sinan Reyhan <enissinanreyhan@gmail.com>\n * @data {boolean} loginfailed - username and password data is recorded\n * @data {object} form - form data is kept \n */\n  export default {\n    name:'signin',\n    \n   \n       data () {\n      return {\n        form:{\n          username:'',\n          password:'',\n          \n        },\n        loginfailed:false\n      }\n      },\n      computed: {\n        ...mapState({\n        authorized: state => state.auth.user\n      \n      })\n      },\n      methods: {\n        ...mapActions({\n            signIn: 'auth/signIn',\n            authenticated:'auth/authenticated',\n            user:'auth/authenticated'\n        }),\n         /**\n         * The user data is checked in the function upon submit button.\n         * If registration is successful, the user is redirected to the ProductList component. \n         */\n       redirect () {\n\n          if(this.authorized) {\n          this.$router.replace({\n                name: 'ProductList'\n              }) \n              } else {\n                this.loginfailed = true\n          }\n\n        },\n\n        /**\n         * \n         * if the axios call in the signIn vuex actions is successful, the redirect function is called. \n         */\n        async submit(){ \n\n         await this.signIn(this.form).then(() =>{\n          this.redirect()\n         })\n         \n           \n\n              \n            \n          }\n\n        \n          \n        \n      \n\n      }\n     \n\n    \n  }\n</script>"],"sourceRoot":"src/pages"}]}