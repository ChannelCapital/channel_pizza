\"width:90%, padding-left:10 px\">\n\n<h1> Products </h1>\n\n<table class=\"table\">\n  <thead>\n    <tr>\n      <th scope=\"col\">Id</th>\n      <th scope=\"col\">Title</th>\n      <th scope=\"col\">Description</th>\n      <th scope=\"col\">Price</th>\n      <th scope=\"col\"> Image Source</th>\n      <th scope=\"col\">Type</th>\n      <th scope=\"col\">Delete</th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr v-for=\"product in products\" :key=\"product.id\">\n      <td >{{product.id}}</td>\n      <td>{{product.title}}</td>\n      <td>{{product.description}}</td>\n      <td>{{product.price}}</td>\n      <td>> {{product.image}}</td>\n      <td>{{product.type}}</td>\n      <td> <button id=\"submitButton\" type=\"button\" class=\"btn btn-success btn-sm\" @click=\"deleteProduct(product.id)\">Delete</button> </td>\n    </tr>\n  </tbody>\n</table>\n\n<router-link class=\"nav-link\" :to=\"'/admin/addproduct'\">Add New Product</router-link> \n\n</div>\n\n</template>\n<script>\nimport axios from 'axios'\nexport default {\n \n      \n\n     mounted() {\n    this.$store.dispatch('getProducts')\n},\n\n\n    computed : {\n        products () {\n            return this.$store.state.products;\n\n        },\n        \n        \n    },\n    methods : {\n   \n      async deleteProduct (id) {\n      console.log(\"product\" + id)\n      \n    await axios.delete('http://localhost:8080/api/messages/deleteProduct/'+id)\n.then(response =>{\n    if(response.data){\n    this.$store.dispatch('getProducts') }\n             \n}).catch(error => {\n  console.log(error)\n})\n\n      }\n    }\n    \n}\n</script>\n"],"sourceRoot":"src/pages/admin"}]}