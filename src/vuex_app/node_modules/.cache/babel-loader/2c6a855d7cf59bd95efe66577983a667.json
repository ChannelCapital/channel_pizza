JF,EAIQS,QAJR,EAIiB;AACpBT,MAAAA,KAAK,CAACE,IAAN,GAAaO,QAAb;AACH,KANM;AAOPC,IAAAA,SAPO,qBAOGV,KAPH,EAOSW,SAPT,EAOmB;AACtBX,MAAAA,KAAK,CAACG,KAAN,GAAcQ,SAAd;AACH,KATM;AAUPC,IAAAA,OAVO,mBAUEZ,KAVF,EAUS;AACZA,MAAAA,KAAK,CAACC,EAAN,GAAW,IAAX,EACAD,KAAK,CAACE,IAAN,GAAa,IADb;AAEAF,MAAAA,KAAK,CAACG,KAAN,GAAc,IAAd;AACAH,MAAAA,KAAK,CAACa,IAAN,GAAY,EAAZ;AACH;AAfM,GAvCA;AAyDXC,EAAAA,OAAO,EAAE;AACL;AACR;AACA;AACA;AACA;AACA;AACcC,IAAAA,MAPD,wBAOmBC,WAPnB,EAOgC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAtBC,gBAAAA,MAAsB,QAAtBA,MAAsB;AAAA;AAAA,uBAEZnB,KAAK,CAACoB,IAAN,CAAW,yCAAX,EAAsDF,WAAtD,CAFY;;AAAA;AAE7BG,gBAAAA,QAF6B;;AAIjC,oBAAIA,QAAQ,CAACC,IAAT,IAAiB,IAArB,EAA0B;AACtBH,kBAAAA,MAAM,CAAC,UAAD,EAAYE,QAAQ,CAACC,IAAT,CAAcX,QAA1B,CAAN;AACAQ,kBAAAA,MAAM,CAAC,QAAD,EAAUE,QAAQ,CAACC,IAAT,CAAcnB,EAAxB,CAAN;AACAgB,kBAAAA,MAAM,CAAC,WAAD,EAAaE,QAAQ,CAACC,IAAT,CAAcjB,KAA3B,CAAN;AACH;;AARgC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAShC,KAhBA;;AAkBL;AACR;AACA;AACA;AACA;AACA;AACckB,IAAAA,QAxBD,2BAwBqBC,WAxBrB,EAwBkC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAtBL,gBAAAA,MAAsB,SAAtBA,MAAsB;AAAA;AAAA,uBACdnB,KAAK,CAACoB,IAAN,CAAW,4CAAX,EAAyDI,WAAzD,CADc;;AAAA;AAC/BH,gBAAAA,QAD+B;;AAEnC,oBAAIA,QAAQ,CAACC,IAAT,IAAiB,IAArB,EAA0B;AACtBH,kBAAAA,MAAM,CAAC,UAAD,EAAYE,QAAQ,CAACC,IAAT,CAAcX,QAA1B,CAAN;AACAQ,kBAAAA,MAAM,CAAC,QAAD,EAAUE,QAAQ,CAACC,IAAT,CAAcnB,EAAxB,CAAN;AACH;;AALkC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOtC,KA/BI;;AAgCJ;AACT;AACA;AACA;AACA;AACQsB,IAAAA,OArCK,0BAqCc;AAAA,UAATN,MAAS,SAATA,MAAS;AACfA,MAAAA,MAAM,CAAC,SAAD,CAAN;AACH;AAvCI;AAzDE,CAAf","sourcesContent":["import axios from 'axios'\r\n/**\r\n * Single vuexs file for User information module\r\n *@module\r\n * @name Vuex - User\r\n *\r\n *\r\n *@type{object}\r\n *@author Enis Sinan Reyhan <enissinanreyhan@gmail.com>\r\n */\r\n\r\nexport default {\r\n    namespaced:true,\r\n\r\n    /**\r\n     * @property {String} id - keeps the userid\r\n     * @property {String} user - keeps the username\r\n     */\r\n    state: {\r\n        id: null,\r\n        user:null,\r\n        admin:null\r\n\r\n    },\r\n    /**\r\n     * @getter {String} authenticated=id Returns id\r\n     * @getter {String} user=user Return user\r\n     * @getter {String} admin=admin Return admnin\r\n     */\r\n    getters: {\r\n\r\n        authenticated (state) {\r\n            return state.id\r\n        },\r\n\r\n        user (state) {\r\n            return state.user\r\n        },\r\n        admin (state) {\r\n            return state.admin\r\n        }\r\n\r\n    },\r\n    /**\r\n     * @mutator {String} SET_ID=id Sets the user id\r\n     * @mutator {String} SET_USER=username Sets the user \r\n     * @mutator {none} LOG_OUT=id,user clears the user and id information\r\n     * @mutator {String} SET_ADMIN =admin sets the admin property\r\n     */\r\n    \r\n    mutations: {\r\n        SET_ID(state,id) {\r\n            state.id= id\r\n        },\r\n        SET_USER(state,username){\r\n            state.user = username\r\n        },\r\n        SET_ADMIN(state,adminuser){\r\n            state.admin = adminuser\r\n        },\r\n        LOG_OUT (state) {\r\n            state.id = null,\r\n            state.user = null\r\n            state.admin = null\r\n            state.cart= []\r\n        }\r\n        \r\n    },\r\n    actions: {\r\n        /**\r\n         * Checks the user information, if successful, commits the data \r\n        * @method signIn\r\n        * @param {object} credentials - user information \r\n        * @returns {void}\r\n         */\r\n        async signIn ({commit}, credentials) {\r\n\r\n            let response = await axios.post('http://localhost:8080/api/messages/user', credentials)\r\n            \r\n            if (response.data != null){\r\n                commit('SET_USER',response.data.username)\r\n                commit('SET_ID',response.data.id)\r\n                commit('SET_ADMIN',response.data.admin)\r\n            } \r\n            },  \r\n\r\n        /**\r\n         * Checks the user information, if successful, commits the data \r\n        * @method register\r\n        * @param {object} information - user information \r\n        * @returns {void}\r\n         */\r\n        async register ({commit}, information) {\r\n            let response = await axios.post('http://localhost:8080/api/messages/addUser', information)\r\n            if (response.data != null){\r\n                commit('SET_USER',response.data.username)\r\n                commit('SET_ID',response.data.id)\r\n            } \r\n        \r\n        },\r\n         /**\r\n         * Checks the user information, if successful, commits the data \r\n        * @method logOut\r\n        * @returns {void}\r\n         */\r\n        signout ({commit}) {\r\n            commit('LOG_OUT')\r\n        }\r\n        \r\n        }\r\n    }\r\n"]}]}